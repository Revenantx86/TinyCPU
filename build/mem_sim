#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 9;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2009.vpi";
S_0x5c3d46f2b960 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x5c3d46f3b2c0 .scope module, "tb_inst_mem" "tb_inst_mem" 3 4;
 .timescale -9 -9;
v0x5c3d46f54ef0_0 .var "address", 7 0;
v0x5c3d46f54fb0_0 .var/i "errors", 31 0;
v0x5c3d46f55070_0 .net "instruction", 15 0, L_0x5c3d46f55400;  1 drivers
S_0x5c3d46f3b450 .scope module, "uut" "inst_mem" 3 12, 4 1 0, S_0x5c3d46f3b2c0;
 .timescale 0 0;
    .port_info 0 /INPUT 8 "address";
    .port_info 1 /OUTPUT 16 "instruction";
L_0x5c3d46f55400 .functor BUFZ 16, L_0x5c3d46f55170, C4<0000000000000000>, C4<0000000000000000>, C4<0000000000000000>;
v0x5c3d46f3b5e0_0 .net *"_ivl_0", 15 0, L_0x5c3d46f55170;  1 drivers
v0x5c3d46f54a20_0 .net *"_ivl_2", 9 0, L_0x5c3d46f55270;  1 drivers
L_0x7528477b7018 .functor BUFT 1, C4<00>, C4<0>, C4<0>, C4<0>;
v0x5c3d46f54b00_0 .net *"_ivl_5", 1 0, L_0x7528477b7018;  1 drivers
v0x5c3d46f54bc0_0 .net "address", 7 0, v0x5c3d46f54ef0_0;  1 drivers
v0x5c3d46f54ca0_0 .net "instruction", 15 0, L_0x5c3d46f55400;  alias, 1 drivers
v0x5c3d46f54dd0 .array "memory", 255 0, 15 0;
L_0x5c3d46f55170 .array/port v0x5c3d46f54dd0, L_0x5c3d46f55270;
L_0x5c3d46f55270 .concat [ 8 2 0 0], v0x5c3d46f54ef0_0, L_0x7528477b7018;
    .scope S_0x5c3d46f3b450;
T_0 ;
    %vpi_call/w 4 12 "$readmemh", "program.hex", v0x5c3d46f54dd0 {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x5c3d46f3b2c0;
T_1 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5c3d46f54fb0_0, 0, 32;
    %end;
    .thread T_1, $init;
    .scope S_0x5c3d46f3b2c0;
T_2 ;
    %vpi_call/w 3 19 "$display", "\012--- Starting Instruction Memory Verification ---" {0 0 0};
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x5c3d46f54ef0_0, 0, 8;
    %delay 5, 0;
    %load/vec4 v0x5c3d46f55070_0;
    %cmpi/ne 4386, 0, 16;
    %jmp/0xz  T_2.0, 6;
    %vpi_call/w 3 25 "$display", "\342\235\214 FAIL: Addr 0. Expected 1122, Got %h", v0x5c3d46f55070_0 {0 0 0};
    %load/vec4 v0x5c3d46f54fb0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5c3d46f54fb0_0, 0, 32;
    %jmp T_2.1;
T_2.0 ;
    %vpi_call/w 3 28 "$display", "\342\234\205 PASS: Addr 0 Correct (1122)" {0 0 0};
T_2.1 ;
    %pushi/vec4 2, 0, 8;
    %store/vec4 v0x5c3d46f54ef0_0, 0, 8;
    %delay 5, 0;
    %load/vec4 v0x5c3d46f55070_0;
    %cmpi/ne 57005, 0, 16;
    %jmp/0xz  T_2.2, 6;
    %vpi_call/w 3 35 "$display", "\342\235\214 FAIL: Addr 2. Expected DEAD, Got %h", v0x5c3d46f55070_0 {0 0 0};
    %load/vec4 v0x5c3d46f54fb0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5c3d46f54fb0_0, 0, 32;
    %jmp T_2.3;
T_2.2 ;
    %vpi_call/w 3 38 "$display", "\342\234\205 PASS: Addr 2 Correct (DEAD)" {0 0 0};
T_2.3 ;
    %pushi/vec4 3, 0, 8;
    %store/vec4 v0x5c3d46f54ef0_0, 0, 8;
    %delay 5, 0;
    %load/vec4 v0x5c3d46f55070_0;
    %cmpi/ne 48879, 0, 16;
    %jmp/0xz  T_2.4, 6;
    %vpi_call/w 3 45 "$display", "\342\235\214 FAIL: Addr 3. Expected BEEF, Got %h", v0x5c3d46f55070_0 {0 0 0};
    %load/vec4 v0x5c3d46f54fb0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5c3d46f54fb0_0, 0, 32;
    %jmp T_2.5;
T_2.4 ;
    %vpi_call/w 3 48 "$display", "\342\234\205 PASS: Addr 3 Correct (BEEF)" {0 0 0};
T_2.5 ;
    %load/vec4 v0x5c3d46f54fb0_0;
    %cmpi/s 0, 0, 32;
    %flag_or 5, 4; GT is !LE
    %flag_inv 5;
    %jmp/0xz  T_2.6, 5;
    %vpi_call/w 3 53 "$display", "\012\360\237\224\264 FAILED: Found %0d errors.", v0x5c3d46f54fb0_0 {0 0 0};
    %vpi_call/w 3 54 "$finish", 32'sb00000000000000000000000000000001 {0 0 0};
    %jmp T_2.7;
T_2.6 ;
    %vpi_call/w 3 56 "$display", "\012\360\237\237\242 SUCCESS: Instruction Memory works!" {0 0 0};
    %vpi_call/w 3 57 "$finish", 32'sb00000000000000000000000000000000 {0 0 0};
T_2.7 ;
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 5;
    "N/A";
    "<interactive>";
    "-";
    "test/tb_inst_mem.v";
    "src/inst_mem.v";
